<div class="panel panel-default">
  <!-- <div class="panel-heading"><h3 class="panel-title">{{model.groupName}}</h3></div>  -->
  <div class="panel-body">
    <dl class="dl-horizontal">
      <dt>Group Name</dt>
      <dd>{{model.groupName}}</dd>
      <dt>Admin</dt>
      <dd>
        <ul class="list-unstyled">
          <li>{{model.createdBy.name}} <span class="label label-default">{{model.createdBy.mbti}}</span></li>
          <li><a href="mailto:{{model.createdBy.email}}">{{model.createdBy.email}}</a></li>
        </ul>
      </dd>
      <dt>Members</dt>
      <dd>
        <ul class="list-unstyled">
          {{#each model.members as |member|}}
            <li>{{member.name}} <span class="label label-default">{{member.mbti}}</span></li>
            <li><a href="mailto:{{member.email}}">{{member.email}}</a></li>
          {{/each}}
        </ul>
      </dd>
    </dl>
  </div>
</div>
